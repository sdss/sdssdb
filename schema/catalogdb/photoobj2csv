indexList = []
for col in wantCols:
    indexList.append(allCols.index(col))

PH_DIR = "/uufs/chpc.utah.edu/common/home/sdss/dr13/casload/phCSV/2"

fileList = []
for dirpath,n,fs in os.walk(PH_DIR):
    for f in fs:
        if f.startswith("sqlPhotoObjAll"):
            fileList.append(os.path.join(dirpath,f))

def writeCSV(casCSV):
    # cant pass out dir using multiprocessing?
    outFilePath = casCSV.split("/")[-1].split(".bz2")[0]
    outFilePath = os.path.join("/scratch/general/lustre/u0449727/photoobj", outFilePath)
    outfile = open(outFilePath, "w")
    cf = bz2.BZ2File(casCSV, "r")
    for line in cf:
        vals = line.split(",")
        outVals = []
        for index in indexList:
            outVals.append(vals[index])
        outLine = ",".join(outVals) + "\r\n"
        outfile.write(outLine)
    cf.close()
    outfile.close()
    print("wrote: ", outFilePath)

pool = Pool(processes=24)
pool.map(writeCSV, fileList)


